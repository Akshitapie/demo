{
    "version": 2,
    "builds": [
        {
            "src": "demo/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.8"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "demo/wsgi.py"
        }
    ]
}