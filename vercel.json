{
  "version": 2,
  "builds": [
    {
      "src": "demo/wsgi.py",
      "use": "@vercel-python-wsgi",
      "config": {
        "runtime": "python3.9",
        "output": "dist" 
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "demo/wsgi.py"
    }
  ]
}
